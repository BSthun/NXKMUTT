{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Events",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "type": "string",
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "conference",
        "seminar",
        "meeting"
      ],
      "required": true
    },
    "multidays": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "date_start": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "datetime",
      "required": true
    },
    "date_end": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "datetime",
      "required": true
    },
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "desc": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "attachment.link"
    },
    "organizers": {
      "collection": "members"
    }
  }
}
