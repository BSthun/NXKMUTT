{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "name": "members",
    "description": "Members and staffs in the organization."
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name_en": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "maxLength": 32,
      "minLength": 8
    },
    "name_th": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "maxLength": 32,
      "minLength": 8
    },
    "nickname_en": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "maxLength": 10,
      "minLength": 2
    },
    "nickname_th": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "maxLength": 10,
      "minLength": 2
    },
    "username": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 4,
      "maxLength": 16
    },
    "position": {
      "pluginOptions": {},
      "type": "string",
      "required": true,
      "maxLength": 16,
      "minLength": 4
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "contact": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {},
      "component": "profile.contact"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {},
      "component": "attachment.link"
    },
    "affiliation": {
      "model": "affiliations"
    },
    "attrs": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {},
      "component": "profile.attribute"
    }
  }
}
